{
  "manifest_version": 3,
  "name": "Custom Speed",
  "description": "Controls speed of your video",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "/icons/icon_128.png"
    }
  },
  "background":{
    "service_worker": "/scripts/background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "/icons/icon_128.png"
  }
}